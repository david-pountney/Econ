    //Makes sure only ONE menu at a time
    if(menu != noone &amp;&amp; militaryRecruitMenuOpen){
        militaryRecruitMenuOpen = false;  
    }
    
    //Cancel any businessMarker, we are starting again
    if(instance_exists(obj_businessMarker))
